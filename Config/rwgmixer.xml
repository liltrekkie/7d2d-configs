<?xml version="1.0" encoding="utf-8"?>

<rwgmixer>

	<world ruleset="vanilla" />
	
	<rulesets> <!-- if having trouble loading try changing the cell_size to "2000" or "2200" -->
		<ruleset name="vanilla" cell_cache_size="8" cell_size="1800" cell_offset="0" generation_distance="10000" terrain_generator="vanilla" biome_generator="vanilla">
		
			<cell_rule name="default" prob="1"/>
			<!--<cell_rule name="customHubTest" position="0,1" prob="0"/>-->
			
		</ruleset>
		
	</rulesets>
	
	<cell_rules>
	
		<cell_rule name="default">
		
			<cave_count value="0,1"/>
			<path_material value="asphalt" /> 
			<path_radius value="7" />
			
			<!-- <hub_rule name="rural" prob="1"/> -->
			<hub_rule name="Test" prob="1"/>
			
			<wilderness_rule name="wildernessDefault" prob="1"/>
			
		</cell_rule>
		
	</cell_rules>
	
	<hub_rules>
	
		<hub_rule name="rural">
		
			<hub_type value="rural"/>
			<width value="100, 100" />
			<height value="100, 100" />
			<path_material value="asphalt" /> 
			<path_radius value="10" />

			<prefab_rule name="default"/>
			
			<street_gen level="10" length_multiplier="4">
			
				<axiom value="[F]"/>
				<rule char="J" replace_with="[KYX*]"/>
				<rule char="K" replace_with="[F+F-F]"/>
				<rule char="L" replace_with="[*F]"/>
				<rule char="Y" replace_with="-FF[YX]*"/>
				<rule char="X" replace_with="+FF[XY]*"/>
				<alt_commands chars="J,K,L,Y,X"/>
				
			</street_gen>
			
		</hub_rule>
		
		<hub_rule name="Test">
		
			<hub_type value="city"/>
			<width value="1820, 1820" />
			<height value="1820, 1820" />
			<path_material value="asphalt" /> 
			<path_radius value="7" />
			
			<downtown_zone_size_perc value="0.75"/>
			
			<prefab_rule name="city"/>
			
			<street_gen level="76" length_multiplier="6">  
			
                <axiom value="[FFF[T]FFF[T]FFF]+[FFF[T]FFF[T]FFF]+[FFF[T]FFF[T]FFF]+[FFF[T]FFF[T]FFF]+[FFF[T]FFF[T]FFF]"/>
				
                    <rule char="T" replace_with="-FFFFF-[FFFFFI]+[FFFFFI]+[FFFFFI]+[FFFFFI]+[FFFFFI]"/>
                    <rule char="I" replace_with="[FFF[T]FFF[T]FFF]+[FFF[T]FFF[T]FFF]+[FFF[T]FFF[T]FFF]+[FFF[T]FFF[T]FFF]"/>
                    <alt_commands chars="T"/>
			
            </street_gen>
			
		</hub_rule>
		
	</hub_rules>
	
	<wilderness_rules>
	
		<wilderness_rule name="wildernessDefault" spawn_min_max="12,24">	<!-- If you want more/less wilderness pois increase spawn_min_max="0,0" max is "60,60" thats 60 wilderness pois per cell -->
		
			<path_material value="gravel" /> 
			<path_radius value="2" />
			<path_adjust_terrain value="false"/>
			
			<prefab_rule name="wildernessGroup" prob="1"/>
			
		</wilderness_rule>
		
	</wilderness_rules>
	
